package lv0;

public class _037접두사인지확인하기 {
    public static String solution(String my_string, String is_prefix) {
    	boolean answer = false;
    	if (my_string.length() >= is_prefix.length()) {
    		answer = my_string.substring(0, is_prefix.length()) == is_prefix;
    	} 
    	return answer;
    }

	public static void main(String[] args) {
		String my_string = "banana";
		String is_prefix = "ban";
		System.out.println(solution(my_string, is_prefix));
	}
}
