package lv0;

// https://school.programmers.co.kr/learn/courses/30/lessons/181937
public class n의배수 {
	
    public static int solution(int num, int n) {
        return num % n == 0 ? 1 : 0;
    }

	public static void main(String[] args) {
		int num = 34;
		int n = 3;
		System.out.println(solution(num, n));
	}
}
